package Default;

import java.util.Stack;

public class Rever {
public static void main(String args[])
{
	String s="welcome";
	Stack s1=new Stack();
	for(int i=0;i<s.length();i++)
	{
		char c=s.charAt(i);
		s1.push(c);
		
	}
	while(!s1.isEmpty())
	{
		char ch=(Character) s1.pop();
		
		System.out.print(ch);
	}
}
}
